{
  "manifest_version": 2,
  "name": "Fantasy Assistant",
  "version": "1.0",
  "description": "A Chrome extension to help users with their Draft Fantasy Premier League Football.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo1NoBG16.png",
      "48": "logo1NoBG48.png",
      "128": "logo1NoBG.png"
    }
  },
  "permissions": ["activeTab"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
